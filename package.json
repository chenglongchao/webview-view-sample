{
    "name": "name-plug",
    "displayName": "Name Plug",
    "description": "A sample VSCode extension",
    "version": "0.0.1",
    "publisher": "your-name",
    "engines": {
        "vscode": "^1.60.0"
    },
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "calicoColors",
                    "title": "反向查找引用",
                    "icon": "resources/icon.svg"
                }
            ]
        },
        "views": {
            "calicoColors": [
                {
                    "type": "webview",
                    "id": "calicoColors.colorsView",
                    "name": ""
                }
            ]
        },
        "commands": [
            {
                "command": "calicoColors.customCommand",
                "title": "反向查找引用"
            },
            {
                "command": "calicoColors.reverseLookup",
                "title": "反向查找引用"
            }
        ],
        "menus": {
            "view/title": [],
            "editor/context": [
                {
                    "command": "calicoColors.customCommand",
                    "when": "resourceExtname == '.tsx' || resourceExtname == '.ts' || resourceExtname == '.js' || resourceExtname == '.jsx'"
                }
            ],
            "explorer/context": [
                {
                    "command": "calicoColors.reverseLookup",
                    "group": "navigation",
                    "when": "resourceExtname == '.tsx' || resourceExtname == '.ts' || resourceExtname == '.js' || resourceExtname == '.jsx'"
                }
            ],
            "editor/title": [
                {
                    "command": "calicoColors.reverseLookup",
                    "when": "editorHasDocumentSymbolProvider"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@stylistic/eslint-plugin": "^2.9.0",
        "@types/node": "^22.10.5",
        "@types/vscode": "^1.96.0",
        "eslint": "^9.13.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.16.0"
    },
    "dependencies": {
        "clc_quote": "^1.0.1",
        "fs": "*",
        "path": "^0.12.7"
    }
}
